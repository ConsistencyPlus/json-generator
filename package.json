{
    "name": "mod-json-generator",
    "version": "1.0.0",
    "author": "deviouslrd",
    "description": "Auto-creates the specified JSON files for a minecraft mod.",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "build": "electron-packager ./ \"Minecraft JSON Generator\" --out=builds --executable-name=minecraftjsongenerator --appname=\"Minecraft JSON Generator\" --app-version=1.6 --ignore=/builds --ignore=/node_modules --ignore=.gitignore --ignore=README.md --ignore=.eslintrc.json --icon=./assets/icon.ico"
    },
    "devDependencies": {
        "electron": "^11.1.1",
        "electron-packager": "^15.2.0"
    }
}